{
  "name": "feathers-ekosystem",
  "description": "A collection of Kalisio maintained modules for FeathersJS",
  "packageManager": "pnpm@10.1.0",
  "type": "module",
  "standard": {
    "globals": [
      "describe",
      "it",
      "expect",
      "beforeEach",
      "afterEach",
      "Image"
    ],
    "ignore": [
      "dist/**"
    ]
  },
  "lint-staged": {
    "*.{js,cjs,mjs}": [
      "pnpm lint"
    ]
  },
  "scripts": {
    "prepare": "husky",
    "lint": "standard 'packages/**/*.js' --fix",
    "lint:feathers-keycloak-listener": "pnpm run lint -- 'packages/feathers-keycloak-listener/**/*.js'",
    "test": "vitest run --coverage",
    "test:feathers-keycloak-listener": "pnpm run test --project feathers-keycloak-listener",
    "build": "pnpm run build:feathers-keycloak-listener",
    "build:feathers-keycloak-listener": "pnpm --filter @kalisio/feathers-keycloak-listener build",
    "changeset": "changeset",
    "changeset:status": "changeset status",
    "changeset:version": "changeset version",
    "changeset:release": "pnpm build && changeset publish",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "keycloak",
    "feathers",
    "ekosystem",
    "kalisio"
  ],
  "author": {
    "name": "KALISIO <contact@kalisio.com>",
    "url": "https://github.com/kalisio"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kalisio/feathers-ekosystem.git"
  },
  "bugs": {
    "url": "https://github.com/kalisio/feathers-ekosystem/issues"
  },
  "homepage": "git://github.com/kalisio/feathers-ekosystem#readme",
  "dependencies": {
    "keycloak-js": "catalog:",
    "lodash": "catalog:",
    "moment": "catalog:",
    "quasar": "catalog:"
  },
  "devDependencies": {
    "@changesets/cli": "catalog:",
    "@kalisio/meta-ekosystem": "^0.2.0",
    "@vitest/coverage-v8": "catalog:",
    "cross-env": "catalog:",
    "husky": "catalog:",
    "lint-staged": "catalog:",
    "mermaid": "catalog:",
    "standard": "catalog:",
    "vite": "catalog:",
    "vitepress": "catalog:",
    "vitepress-plugin-mermaid": "catalog:",
    "vitepress-theme-kalisio": "catalog:",
    "vitest": "catalog:"
  },
  "pnpm": {
    "overrides": {
      "@kalisio/meta-ekosystem": "link:../meta-ekosystem"
    }
  }
}
